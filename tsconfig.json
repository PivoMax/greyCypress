{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "importHelpers": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "rootDir": ".",
    "baseUrl": "./src",
    "types": ["cypress", "cypress-real-events"],
    "paths": {
      "$contentful-schema": ["generated/contentful/schema.gql"],
      "$contentful-schema-react": ["generated/contentful/schema.react-query.gql"],
      "$components/*": ["components/*"],
      "$content/*": ["content/*"],
      "$queries/*": ["queries/*"],
      "$stories/*": ["stories/*"],
      "$assets/*": ["assets/*"],
      "$utils/*": ["utils/*"]
    },
    "typeRoots": ["./node_modules/@types", "./typings"]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "cypress/utils/accessibilityTerminalLogs.js", "cypress/utils/propsindex.js"  ],
  "exclude": [
    "node_modules"
  ]
}
